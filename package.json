{
  "name": "@bekk/storybook",
  "version": "1.9.56",
  "description": "Storybokk for Bekk Consulting As",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "keywords": [
    "Bekk",
    "Storybook",
    "Typescript"
  ],
  "author": "Morten Holst Øvrebø",
  "license": "MIT",
  "files": [
    "build/lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bekk/komponent-bibliotek"
  },
  "bugs": {
    "url": "https://github.com/bekk/komponent-bibliotek/issues"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build": "npm run build-lib && build-storybook",
    "build-lib": "tsc && npm run copy-assets-to-lib",
    "build-storybook": "build-storybook",
    "copy-assets-to-lib": "npm run copy-css-to-lib && npm run copy-svg-to-lib && npm run copy-fonts-to-lib",
    "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build/lib",
    "copy-svg-to-lib": "cpx \"./src/**/*.svg\" ./build/lib",
    "copy-fonts-to-lib": "cpx \"./src/**/*.{eot,ttf,woff,woff2}\" ./build/lib"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@storybook/addon-backgrounds": "^3.4.11",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/react": "^4.0.0-alpha.21",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/node": "^10.11.5",
    "@types/react": "^16.3.5",
    "@types/storybook__addon-backgrounds": "^3.2.1",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__react": "^3.0.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "node-sass": "^4.9.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "sass-loader": "^7.1.0",
    "storybook-addon-jsx": "^5.4.0",
    "ts-loader": "^5.2.1",
    "typescript": "^2.8.1",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "@types/react-dates": "^17.1.2",
    "@types/react-select": "^2.0.5",
    "@types/styled-components": "^4.0.1",
    "autoprefixer": "^9.1.5",
    "css-loader": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.3.1",
    "react-dates": "^18.1.1",
    "react-dom": "^16.3.1",
    "react-select": "^2.1.0",
    "styled-components": "^4.0.2"
  }
}
